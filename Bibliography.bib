doi = {10.1016/j.jpowsour.2009.11.129},
file = {:Users/pstanley/Documents/Mendeley Desktop/Meille et al/Journal of the European Ceramic Society/Meille et al. - Mechanical properties of porous ceramics in compression On the transition between elastic, brittle, and cellular behavio.pdf:pdf},
isbn = {0378-7753},
issn = {03787753},
journal = {Journal of Power Sources},
keywords = {SOFC,Start-up,Thermal dynamics,Transient performance},
month = {may},
number = {10},
pages = {3186--3200},
title = {{Computational model to predict thermal dynamics of planar solid oxide fuel cell stack during start-up process}},
url = {http://linkinghub.elsevier.com/retrieve/pii/S0378775309021375},
volume = {195},
