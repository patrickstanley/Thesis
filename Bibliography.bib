pages = {13612--13623},
publisher = {Royal Society of Chemistry},
title = {{Thermodynamic and kinetic assessments of strontium-doped lanthanum manganite perovskites for two-step thermochemical water splitting}},
url = {http://xlink.rsc.org/?DOI=C4TA02694B},
volume = {2},
year = {2014}
}
