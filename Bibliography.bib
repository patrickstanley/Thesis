journal = {Solid State Ionics},
keywords = {ceramic electrolyte,ceria,electrical conductivity,it-sofc,microstructure,thermodynamics},
number = {1-4},
pages = {95--110},
pmid = {20728971},
title = {{Appraisal of Ce1−yGdyO2−y/2 electrolytes for IT-SOFC operation at 500°C}},
