isbn = {0167-2738},
issn = {01672738},
journal = {Solid State Ionics},
keywords = {conductivity,defect modelling,lsm,non stoichiometry,perovskite,sequential algorithm},
number = {1},
pages = {145--162},
pmid = {84},
title = {{Defect chemistry modelling of oxygen-stoichiometry, vacancy concentrations, and conductivity of (La1-xSrx)yMnO3±$\delta$}},
