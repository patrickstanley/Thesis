journal = {Journal of the European Ceramic Society},
keywords = {Creep,Failure mechanisms,Fracture,Fuel cells,ZrO2},
number = {6},
pages = {751--760},
title = {{Room and high temperature failure mechanisms in solid oxide fuel cell electrolytes}},
url = {http://www.sciencedirect.com/science/article/pii/S0955221999000801},
volume = {20},
year = {2000}
}
