@book{Ishihara2009,
abstract = {Fuel cell technology is quite promising for conversion of chemical energy of hydrocarbon fuels into electricity without forming air pollutants. There are several types of fuel cells: polymer electrolyte fuel cell (PEFC), phosphoric acid fuel cell (PAFC), molten carbonate fuel cell (MCFC), solid oxide fuel cell (SOFC), and alkaline fuel cell (AFC). Among these, SOFCs are the most efficient and have various advantages such as flexibility in fuel, high reliability, simple balance of plant (BOP), and a long history. Therefore, SOFC technology is attracting much attention as a power plant and is now close to marketing as a combined heat and power generation system. From the beginning of SOFC development, many perovskite oxides have been used for SOFC components; for example, LaMnO -based oxide for the cathode and 3 LaCrO for the interconnect are the most well known materials for SOFCs. The 3 current SOFCs operate at temperatures higher than 1073 K. However, lowering the operating temperature of SOFCs is an important goal for further SOFC development. Reliability, durability, and stability of the SOFCs could be greatly improved by decreasing their operating temperature. In addition, a lower operating temperature is also beneficial for shortening the startup time and decreasing energy loss from heat radiation. For this purpose, faster oxide ion conductors are required to replace the conventional Y O -stabilized ZrO 2 3 2 electrolyte. A new class of electrolytes such as LaGaO is considered to be 3 highly useful for intermediate-temperature SOFCs.},
author = {Ishihara, Tatsumi},
booktitle = {Perovskite oxide for solid oxide fuel cells},
doi = {10.1007/978-0-387-77708-5},
editor = {Ishihara, Tatsumi},
file = {:Users/pstanley/Documents/Mendeley Desktop/Ishihara/Perovskite oxide for solid oxide fuel cells/Ishihara - Perovskite Oxide for Solid Oxide Fuel Cells.pdf:pdf},
isbn = {978-0-387-77707-8},
pages = {302},
publisher = {Springer},
title = {{Perovskite Oxide for Solid Oxide Fuel Cells}},
url = {http://link.springer.com/10.1007/978-0-387-77708-5},
year = {2009}
}
@article{Goodenough2007,
abstract = {The electrolyte of a solid oxide fuel cell (SOFC) is an O2--ion conductor. The anode must oxidize the fuel with O2-ions received from the electrolyte and it must deliver electrons of the fuel chemisorption reaction to a current collector. Cells operating on H2and CO generally use a porous Ni/electrolyte cermet that supports a thin, dense electrolyte. Ni acts as both the electronic conductor and the catalyst for splitting the H2bond; the oxidation of H2to H2O occurs at the Ni/electrolyte/H2triple-phase boundary (TPB). The CO is oxidized at the oxide component of the cermet, which may be the electrolyte, yttria-stabilized zirconia, or a mixed oxide-ion/electron conductor (MIEC). The MIEC is commonly a Gd-doped ceria. The design and fabrication of these anodes are evaluated. Use of natural gas as the fuel requires another strategy, and MIECs are being explored for this application. The several constraints on these MIECs are outlined, and preliminary results of this on-going investigation are reviewed. {\textcopyright} 2007 Elsevier B.V. All rights reserved.},
author = {Goodenough, John B. and Huang, Yun Hui},
doi = {10.1016/j.jpowsour.2007.08.011},
file = {:Users/pstanley/Documents/Mendeley Desktop/Goodenough, Huang/Journal of Power Sources/Goodenough, Huang - Alternative anode materials for solid oxide fuel cells.pdf:pdf},
issn = {03787753},
journal = {Journal of Power Sources},
keywords = {Anode materials,Mixed oxide-ion/electron conductor,Solid oxide fuel cells,Yttria-stabilized zirconia},
number = {1},
pages = {1--10},
title = {{Alternative anode materials for solid oxide fuel cells}},
volume = {173},
year = {2007}
}
@article{Hussaina,
abstract = {Our results suggest that an SFCM based ceramic oxide emerges as a unique, redox stable anode-material for LT-SOFCs. Further, SFCM-GDC cells show robust SOFC operation after 30 redox cycles under load at 600°C in H2/3{\%}H2O. Unlike the commonly used Ni-GDC, ceramic anodes retained the dimensional stability and microstructure as that of pristine SFCM-GDC even under harsh cycling conditions. Oxygen stoichiometry of ceramic SFCM plays an important role in maintaining the dimensional stability after multiple redox cycles. The excellent redox stability and performance of SFCM based ceramic anode-supported cell has greater potential to replace Ni based LT-SOFC anodes. The astounding dimensional stability and performance of the SFCM ceramic anode is viewed as significant progress towards replacement of Ni anodes, which would provide a straight path for the commercialization of LT-SOFC technology.},
annote = {Determination of redox stability for an SFCM based anode supported cells},
author = {Hussain, A. Mohammed and Huang, Yi-Lin and Pan, Ke-Ji and Robinson, Ian and Wachsman, Eric D.},
title = {{A Redox-Robust Ceramic Anode for Low-Temperature SOFC}}
}
@article{Pan,
abstract = {Identification of the alternative anode materials for solid oxide fuel cells (SOFCs) is essential to overcome the inherent challenges (e.g. redox instability, coking and sulfur intolerance) associated with Ni-based cermet anodes. However, replacing Ni-cermet is challenging for low-temperature operating SOFCs (LT-SOFCs) employing ceria based mixed ionic and electronic conducting electrolytes. Here we successfully prepared a new perovskite oxide composition (e.g. SrFe0.2Co0.4Mo0.4O3-$\delta$) to function as an alternative anode-support for LT-SOFCs (400-650 oC), exhibiting a high conductivity of 35 S/cm at 600oC. Further, we fabricated anode-supported SOFC with an optimized anode composition. Our LT-SOFC design constitutes of a unique Co-doped GDC layer deposited at the interface of cathode/electrolyte junction, resulting in improved OCV by preventing diffusion of elements from anode to electrolyte. The studied LT-SOFCs showed high peak power densities of 730 mW/cm2 and 300 mW/cm2 at an ambitious low temperatures of 550oC and 450oC, respectively, in humidified H2. Although catalytic activity in ceramic anode was incorporated by infiltrating Ni-GDC solution, stability was not compromised in hydrocarbon gases, we demonstrated stable long-term operation over a period of 380h in CH4–containing gas mixtures at 450oC . High performance is attributed to the improvement in OCV and reduction of parasitic losses (e.g. ohmic/non-ohmic area specific resistances).},
annote = {Identification of SFCM and making an SOFC},
author = {Pan, Ke-Ji and Hussain, A. Mohammed and Wachsman, Eric D.},
title = {{High Performance Low-Temperature Solid Oxide Fuel Cells with SrFe0.2Co0.4Mo0.4O3-$\delta$ based Ceramic Anodes}}
}
@article{Hussain,
abstract = {Development of ceramic anodes for low-temperature solid oxide fuel cells (LT-SOFCs) operating on hydrocarbon fuels is critical for technological advancements. Catalytic activity for fuel oxidation in ceramic anodes e.g., SrFe0.2Co0.4Mo0.4O3 (SFCM) can be introduced by incorporation of Ni-GDC nanoparticles on its surface. However, the composition and the particle size of the infiltrated nanoparticles greatly affect the performance and long-term stability of the SOFCs. In this study, high performance and stable SFCM based ceramic anode-supported SOFCs with Ni-GDC nanoparticles were demonstrated for LT-SOFCs. An optimized Ni-GDC infiltrated SOFC showed peak power density of 0.75, 0.65 and 0.36 W/cm2 at 650, 600 and 550°C, respectively, in humidified H2 and 0.62, 0.39 and 0.22 W/cm2 at 650, 600 and 550°C, respectively, in CH4-containing gases. A stable cell voltage of 0.82V is obtained over 200 h operations at a constant current of 0.2 A/cm2 at 600°C in CH4-containing gases. The results show that highly stable SOFCs can be produced by reducing the quantity of Ni in the Ni-GDC composition and by suppressing the grain growth of Ni nanoparticles.},
annote = {Incorporating catalytic acitivty in SFCM by Ni-GDC infiltrations},
author = {Hussain, A. Mohammed and Robinson, Ian and Huang, Yi-Lin and Pan, Ke-Ji and Wachsman, Eric D.},
title = {{Stable Molybdate-Based Ceramic Anodes for Low-Temperature Solid Oxide Fuel Cells}}
}
@article{Hussain2013,
abstract = {The development of low temperature solid oxide fuel cell (SOFC) anodes by infiltration of Pd/Gd-doped cerium oxide (CGO) electrocatalysts in Nb-doped SrTiO3(STN) backbones has been investigated. Modification of the electrode/electrolyte interface by thin layer of spin-coated CGO (400-500 nm) contributed to a significant improvement in performance of the STN backbones and infiltrated electrodes. The improvement is due to the result of CGO enrichment at the interface. The impedance analysis showed that addition of Pd further increased the electrode reaction rate with a factor 10 with respect to the CGO electrocatalysts. Very low electrode polarization resistances of 0.055 $\Omega$ cm2(after excluding the gas diffusion limitation contribution) and 1.2 $\Omega$ cm2at 600 C and 400 C, respectively, have been obtained in 3{\%} H2/H2O. A gradual decrease in polarization resistance was achieved with increasing loading of Pd-CGO electrocatalyst. The microstructural analysis of the infiltrated Pd-CGO electrocatalyst on STN revealed a homogenous coating of Pd and CGO nanoparticles. {\textcopyright} 2013 Elsevier Ltd. All rights reserved.},
author = {Hussain, A. Mohammed and H{\o}gh, Jens V.T. and Zhang, Wei and Blennow, Peter and Bonanos, Nikolaos and Boukamp, Bernard A.},
